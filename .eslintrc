{
    "env": {
        "browser": true
    },
    "plugins": [
        "react",
        "jest"
    ],
    "extends": [
        "airbnb-base",
        "plugin:react/recommended",
        "eslint:recommended",
        "plugin:jest/recommended"
    ],
    "parser": "babel-eslint",
    "settings": {
        "react": {
        "createClass": "createReactClass",
        "pragma": "React",
        "version": "16.0"
        }
    },
    "rules": {
        "no-console":"off"
    }
}